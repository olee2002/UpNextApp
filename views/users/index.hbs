<div class="food">
    <div class="food">
        <div class="title tlt" data-in-effect="fadeIn">You're</div>
        <br>
        <div class="youare">
            <div class="color animated infinite pulse">Up</div>

            <div class="tlt" data-in-effect="rollIn">Next!</div>
        </div>

    </div>


    <div class="button primary ">
        <a href="/users/new">
            Add New User
        </a>
    </div>



    <div class="profile-card-container">
        <div class="user-cards">
            {{#each users}}
            <a href="/users/{{this._id}}" class="link user-card">
                <div class="user-image">
                    {{#if this.photoUrl}}
                    <img src="{{this.photoUrl}}" alt="{{this.nickName}}'s profile photo"> {{else}}
                    <img src="https://picsum.photos/200/300" alt="{{this.nickName}}'s profile photo"> {{/if}}
                </div>
                {{/each}}
                {{!-- <div class="profile-name">
                    {{this.nickName}}
                </div> --}}

            </a>

        </div>
    </div>